{
  "expo": {
    "name": "TellUs",
    "slug": "TellUs",
    "scheme": "tellus",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "updates": {},
    "assetBundlePatterns": [
      "app/assets/"
    ],
    "ios": {
      "bundleIdentifier": "com.tellus",
      "buildNumber": "1",
      "splash": {
        "resizeMode": "contain",
        "image": "./app/assets/splash/ios/Default-iphone.png",
        "backgroundColor": "#eff0f1"
      }
    },
    "android": {
      "package": "com.tellus",
      "versionCode": 1,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "splash": {
        "resizeMode": "native",
        "backgroundColor": "#eff0f1",
        "ldpi": "./app/assets/splash/android/drawable-port-lpi-screen.png",
        "mdpi": "./app/assets/splash/android/drawable-port-mdpi-screen.png",
        "hdpi": "./app/assets/splash/android/drawable-port-hdpi-screen.png",
        "xhdpi": "./app/assets/splash/android/drawable-port-xhdpi-screen.png",
        "xxhdpi": "./app/assets/splash/android/drawable-port-xxhdpi-screen.png",
        "xxxhdpi": "./app/assets/splash/android/drawable-port-xxxhdpi-screen.png"
      }
    },
    "web": {},
    "extra": {
      "ENVIRONMENT": "dev"
    }
  }
}